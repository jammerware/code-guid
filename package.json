{
  "name": "code-guid",
  "displayName": "CodeGuid",
  "description": "A simple extension that allows the creation of GUIDs (UUIDs) inline. Note that the GUIDs generated by this extension aren't cryptographically secure, so their use in production isn't recommended. They're great for testing or debugging, though.",
  "keywords": [
    "uuid",
    "guid",
    "random"
  ],
  "icon": "images/logo.png",
  "version": "0.9.0",
  "publisher": "vs-code-extension-publish",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codeGuid.generateGuid"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "codeGuid.generateGuid",
        "title": "CodeGuid: Generate GUID"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "CodeGuid configuration",
      "properties": {
        "codeGuid.showNotificationWhenGuidGenerated": {
          "type": "boolean",
          "default": true,
          "description": "Show a notification when a GUID is generated and copied to your clipboard."
        }
      }
    },
    "keybindings": [{
        "command": "codeGuid.generateGuid",
        "key": "alt+shift+g g",
        "mac": "cmd+^+g g",
        "when": "editorTextFocus"
    }]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "copy-paste": "^1.3.0"
  }
}